                "-f/media/jbishop/WD4/brainmets/sunnybrook/RAD NEC/M0003/RELCBV_register.nii",
                "-c/media/jbishop/WD4/brainmets/sunnybrook/RAD NEC/M0003/controlpoints.mat",
